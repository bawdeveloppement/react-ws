#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/dev/Documents/Dev/Javascript/react-ws/server/node_modules/.pnpm/node-gyp-build@3.7.0/node_modules/node-gyp-build/build-test.js/node_modules:/home/dev/Documents/Dev/Javascript/react-ws/server/node_modules/.pnpm/node-gyp-build@3.7.0/node_modules/node-gyp-build/node_modules:/home/dev/Documents/Dev/Javascript/react-ws/server/node_modules/.pnpm/node-gyp-build@3.7.0/node_modules:/home/dev/Documents/Dev/Javascript/react-ws/server/node_modules/.pnpm/node_modules:/home/dev/Documents/Dev/Javascript/react-ws/server/node_modules:/home/dev/Documents/Dev/Javascript/react-ws/node_modules:/home/dev/Documents/Dev/Javascript/node_modules:/home/dev/Documents/Dev/node_modules:/home/dev/Documents/node_modules:/home/dev/node_modules:/home/node_modules:/node_modules:/home/dev/Documents/Dev/Javascript/react-ws/server/node_modules/.pnpm/node_modules/node-gyp-build/build-test.js/node_modules:/home/dev/Documents/Dev/Javascript/react-ws/server/node_modules/.pnpm/node_modules/node-gyp-build/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../node-gyp-build/build-test.js" "$@"
else 
  exec node  "$basedir/../node-gyp-build/build-test.js" "$@"
fi
